<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <TextView
            android:text="TextView"
            android:layout_width="149dp"
            android:layout_height="17dp" android:id="@+id/textView4"
            android:layout_marginLeft="12dp" android:layout_marginStart="12dp"
            app:layout_constraintStart_toEndOf="@+id/imageView4" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="75dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="149dp"
            android:layout_height="17dp" android:id="@+id/textView3"
