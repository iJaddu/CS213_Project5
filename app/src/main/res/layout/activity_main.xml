            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button4"
            android:layout_marginLeft="7dp" android:layout_marginStart="7dp"
            app:layout_constraintStart_toEndOf="@+id/textView4" android:layout_marginEnd="10dp"
            android:layout_marginRight="10dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="70dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>