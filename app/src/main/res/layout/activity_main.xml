    <TextView
            android:text="TextView"
            android:layout_width="149dp"
            android:layout_height="17dp" android:id="@+id/textView"
            android:layout_marginLeft="12dp" android:layout_marginStart="12dp"
            app:layout_constraintStart_toEndOf="@+id/imageView" android:layout_marginBottom="175dp"
            app:layout_constraintBottom_toTopOf="@+id/textView2" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="75dp"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button"
            android:layout_marginLeft="7dp" android:layout_marginStart="7dp"
            app:layout_constraintStart_toEndOf="@+id/textView" android:layout_marginEnd="10dp"
            android:layout_marginRight="10dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="70dp"
            android:layout_marginBottom="144dp" app:layout_constraintBottom_toTopOf="@+id/button2"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button2"
            android:layout_marginLeft="7dp" android:layout_marginStart="7dp"
            app:layout_constraintStart_toEndOf="@+id/textView2" android:layout_marginEnd="10dp"
            android:layout_marginRight="10dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="144dp" app:layout_constraintBottom_toTopOf="@+id/button3"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button3"
            android:layout_marginLeft="7dp" android:layout_marginStart="7dp"
            app:layout_constraintStart_toEndOf="@+id/textView3" android:layout_marginEnd="10dp"
